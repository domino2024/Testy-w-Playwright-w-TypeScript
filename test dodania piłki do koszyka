import { test, expect } from '@playwright/test';

test('test', async ({ page }) => {
  await page.goto('http://www.selenium-shop.pl/');
  await page.getByRole('link', { name: 'Dodaj “Piłka nożna Adidas Euro 2020” do koszyka Piłka nożna Adidas Euro 2020' }).click();
  await page.getByRole('button', { name: ' Dodaj do koszyka' }).click();
  await page.getByRole('link', { name: 'Zobacz koszyk' }).click();
});
